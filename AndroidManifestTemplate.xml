<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="REPLACE1"
    android:versionCode="REPLACE2"
    android:versionName="REPLACE3">
    <!-- NOTE: As of the time of R48 v0.8, R48's release build system is being made into a single script.
         The Gradle build (and precise filesystem structure) is involved, so no more (ab)use of IDEA's build folder.
         The release build system that will get used from now on automates both the main build and the Android build.
         Result: Both this file and strings.xml are now *template* files. -->

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="10" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="gabien.MainActivity" android:immersive="true">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
